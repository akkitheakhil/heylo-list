{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@components/*": [
        "src/shared/components/*"
      ],
    }
  }
}
